<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="News_Manger::news__head" title="News Manager" sortOrder="99">
                  <resource id="News_Manger::Category" title="Category" sortOrder="10">
                    <resource id="News_Manger::Category_save" title="save Category" sortOrder="10"/>
